<odoo>
    <record id="seq_school_trip_request" model="ir.sequence">
        <field name="name">تسلسل طلبات الرحلات المدرسية</field>
        <field name="code">school.trip.request.sequence</field>
        <field name="prefix">TRIP/%(year)s/</field>
        <field name="padding">4</field>
